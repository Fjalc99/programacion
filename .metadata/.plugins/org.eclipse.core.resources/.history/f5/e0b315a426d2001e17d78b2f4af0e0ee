package ejercicio6;

import java.util.Comparator;

public class OrdenarPrecioHora implements Comparator <Trabajador> {

	public int compare (Trabajador t1, Trabajador t2) {
		
		if(t1.getSueldoFinal()/t1.getHorasTrabajadas() < t2.getSueldoFinal()/t2.getHorasTrabajadas()) {
			
			return -1;
		}else {
			if(t1.getSueldoFinal()/t1.getHorasTrabajadas() < t2.getSueldoFinal()/t2.getHorasTrabajadas()) {
				return 1;
			}
		}
		
		return 0;
	}
	
}
