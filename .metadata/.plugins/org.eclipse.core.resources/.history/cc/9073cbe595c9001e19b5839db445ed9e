package ejercicio2;

public class Socio {

	private String nombre;
	private int edad;
	private String dni;
	private double cuota;
	
	
	
	public Socio(String nombre, int edad, String dni, double cuota) {
		super();
		this.nombre = nombre;
		this.edad = edad;
		this.dni = dni;
		this.cuota = cuota;
	}
	
	
	
	
	
	
	
}
